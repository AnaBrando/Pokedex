<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:Pokedex"
             x:Class="Pokedex.MainPage">

    <StackLayout VerticalOptions="Center">
        <Image x:Name="logoPokemon"></Image>
        <Entry Placeholder="Digite seu Pokemon" x:Name="txtPesquisaPokemon"></Entry>
        <Button Text="Pesquisar" Clicked="PesquisarPokemon"></Button>
    </StackLayout>

</ContentPage>
