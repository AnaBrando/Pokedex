             x:Class="Pokedex.DescricaoPokemon">
    <ContentPage.Content>
        <StackLayout>
            <StackLayout Orientation="Horizontal">
                
            </StackLayout>
            <Image x:Name="imgPokemon" WidthRequest="300" HeightRequest="300" >
                
            </Image>
            <Label x:Name="nomePokemon" HorizontalOptions="Center"></Label>
            <Label x:Name="pesoPokemon" HorizontalOptions="Center"></Label>
